{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "airbnb",
  "rules": {
    "no-use-before-define": 0,
    "space-before-function-paren": [2, {"anonymous": "always", "named": "always"}],
    "func-names": 0,
    "generator-star-spacing": 0,
    "react/require-extension": 0,
    "require-yield": "off"
  }
}
